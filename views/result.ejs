<link rel="stylesheet" href="http://yui.yahooapis.com/pure/0.5.0/pure-min.css">
<h3>RESULTS</h3>
<% if(errorMessages.length > 0) { %>
  <h4>Errors</h4>
  <ul>
    <% errorMessages.forEach(function(message){ %>
      <li><%= message %></li>
    <% }); %>
  </ul>
<% }  else { %>
  <p>Congrats, no erros found</p>
<% } %>